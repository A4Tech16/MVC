@model IEnumerable<EFAutoSalon.Buyer>
@{
    ViewBag.Title = "Покупатели";
}
<a asp-action="Create">Добавить покупателя</a>
<p></p>
<a asp-action="CreateC">Добавить машину</a>
<p></p>
<a asp-action="IndexC">Сменить таблицу</a>
<p></p>
<h1>Покупатели</h1>
<table class="table">
    <tr><td>Имя</td><td>Фамилия</td><td>Пол</td></tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Surname</td>
            <td>@item.Gender</td>
        </tr>
    }
</table>

